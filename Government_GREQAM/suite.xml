<suite>
<name>Government_GRECAM</name>
<unittest>
<name>test_Government_read_transfer_notifications</name>
<function>Government_read_transfer_notifications</function>
<xagent>Government</xagent>
<declaration>
<variables>
 <variable>
     <type>double</type>
     <name>monthly_transfer_payment</name>
 </variable>
<variable>
     <type>double</type>
     <name>yearly_transfer_payment</name>
 </variable>
<variable>
     <type>double</type>
     <name>payment_account</name>
 </variable>
</variables>
</declaration>
<fixture>
<Government>
<id>100</id>
<hh_transfer_payment>4000</hh_transfer_payment>
<payment_account>0</payment_account>
<yearly_transfer_payment>0</yearly_transfer_payment>
<monthly_transfer_payment>0</monthly_transfer_payment>
<firm_transfer_payment>5000</firm_transfer_payment>
</Government>
<messages>
<hh_transfer_notification>{{1},{100},{1000}}</hh_transfer_notification>
<firm_transfer_notification>{{100}}</firm_transfer_notification>
</messages>
</fixture>
<expected>
<Government>
<payment_account>-9000.00</payment_account>
<monthly_transfer_payment>9000</monthly_transfer_payment>
<yearly_transfer_payment>9000</yearly_transfer_payment>
</Government>
</expected>
</unittest>

</suite>

